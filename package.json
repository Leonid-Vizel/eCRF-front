{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start:dev": "set NODE_OPTIONS=--max-old-space-size=8192 && webpack serve --env port=3000 mode=development stand=dev server=local",
    "build:dev": "webpack --env mode=production stand=dev",
    "build:prod": "webpack --env mode=production stand=prod",
    "deploy": "node deploy.js",
    "deploy:dev": "npm run build:dev && npm run deploy",
    "deploy:dev:manual": "npm run build:dev && node deploy.js dev",
    "deploy:prod:manual": "npm run build:prod && node deploy.js prod",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "style:lint": "npx stylelint **/*.scss",
    "style:lint:fix": "npx stylelint **/*.scss --fix",
    "prepare": "husky install"
  },
