{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        ["@babel/preset-react", { "runtime": "automatic" }]
    ],
    "plugins": [
        [
          "module-resolver",
          {
            "extensions": [".ts", ".tsx", ".js"],
            "root": ["./src"],
            "alias": {
              "src": "/src",
              "root": "."
            }
          }
        ]
      ]
}

